{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "installCommand": "cd frontend && npm install",
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/.next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/products/:path*",
      "destination": "https://demo-app.pages.dev/api/products/:path*"
    },
    {
      "source": "/api/categories/:path*",
      "destination": "https://demo-app.pages.dev/api/categories/:path*"
    },
    {
      "source": "/api/orders/:path*",
      "destination": "https://demo-app.pages.dev/api/orders/:path*"
    },
    {
      "source": "/api/drivers/:path*",
      "destination": "https://demo-app.pages.dev/api/drivers/:path*"
    },
    {
      "source": "/api/reviews/:path*",
      "destination": "https://demo-app.pages.dev/api/reviews/:path*"
    },
    {
      "source": "/api/pricing-tiers/:path*",
      "destination": "https://demo-app.pages.dev/api/pricing-tiers/:path*"
    },
    {
      "source": "/api/settings/:path*",
      "destination": "https://demo-app.pages.dev/api/settings/:path*"
    },
    {
      "source": "/api/admin/:path*",
      "destination": "https://demo-app.pages.dev/api/admin/:path*"
    },
    {
      "source": "/api/telegram/:path*",
      "destination": "https://demo-app.pages.dev/api/telegram/:path*"
    },
    {
      "source": "/api/upload/:path*",
      "destination": "https://demo-app.pages.dev/api/upload/:path*"
    },
    {
      "source": "/api/media/:path*",
      "destination": "https://demo-app.pages.dev/api/media/:path*"
    }
  ]
}
